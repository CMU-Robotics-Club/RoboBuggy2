<!-- roslaunch buggy main.launch -->

<launch>

    <node name="bnyahaj" pkg="buggy" type="ros_to_bnyahaj.py" output="screen" args="--self_name NAND --other_name SC"/>

    <!-- This is the IMU Right? So it shouldn't be needed -->
    <node name="serial_node2" pkg="rosserial_python" type="serial_node.py">
        <param name="port" type="string" value="/dev/ttyACM1"/>
        <param name="baud" type="int" value="115200"/>
    </node> 

    <node name="foxglove" pkg="foxglove_bridge" type="foxglove_bridge" />
    <node name="telematics" pkg="buggy" type="telematics.py" />
</launch>